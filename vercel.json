{
    "framework": "vite",
    "buildCommand": "vite build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/image/(.*)",
            "destination": "https://bxklsejtopzevituoaxk.supabase.co/storage/v1/object/public/$1"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}